{
    "name": "techworker/pascalcoin-php",
    "description": "A PHP library to work with PascalCoin.",
    "keywords": [
        "PascalCoin",
        "Pascal",
        "Crypto",
        "Currency",
        "CryptoCurrency"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Ansbach",
            "email": "benjaminansbach@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../cryptocurrency",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "ext-curl": "*",
        "ext-json": "*",
        "techworker/cryptocurrency": "dev-master as 1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Techworker\\PascalCoin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Techworker\\PascalCoin\\": "tests"
        }
    }
}
